    <sensor name="camera1" type="camera">
      <update_rate>30.0</update_rate>
      <camera name="head">
        <horizontal_fov>0.7330382858376184</horizontal_fov>
        <image>
          <width>460</width>
          <height>350</height>
          <format>R8G8B8</format>
        </image>
